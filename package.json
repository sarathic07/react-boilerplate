  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn eslint && pretty-quick --staged",
      "pre-push": "yarn eslint"
    }
  }
}
